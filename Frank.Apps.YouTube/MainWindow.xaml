<Window x:Class="Frank.Apps.YouTube.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frank.Apps.YouTube"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBox Name="SearchField"></TextBox>
        <Button Content="Search" Name="SearchButton" Click="SearchButton_OnClick"></Button>
        <ListBox Name="ResultsList"></ListBox>
    </StackPanel>
</Window>
